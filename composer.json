{
    "name": "gmh/tasks-project",
    "description": "Sample project for using Composer and Packagist.",
    "keywords": ["tasks-project", "gmh"],
    "homepage": "https://github.com/GhostMech/TaskProject",
    "license": "MIT",
    "authors": [
        {
            "name": "Gary Howard",
            "email": "garyhowardpctech@yahoo.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "nesbot/carbon": "^2"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "gmh/tasks-project",
                "version": "dev-master",
                "source": {
                    "url": "git://github.com/Ghostmech/TaskProject.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-4" : {
                        "GMH\\" : "src"
                    }
                }
            }
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "phpmd/phpmd": "^2.6",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "GMH\\": "src"
        }
    }
}
